           revision="refs/heads/Rika" />

  <remote name="pixel-devices-blobs"
          fetch="https://gitlab.pixelexperience.org/android/vendor-blobs"
          revision="eleven" />

  <!-- Device repos -->
  <project path="device/canned/sepolicy" name="device_canned_sepolicy" remote="canned" />
